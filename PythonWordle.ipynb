   "outputs": [],
   "source": [
    "#importing dictionary library\n",
    "from PyDictionary import PyDictionary\n",
    "dictionary = PyDictionary()\n",
    "\n",
    "#importing text color library\n",
    "from termcolor import colored\n",
    "\n",
    "#importing wordle list\n",
    "import pandas as pd"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {
    "id": "IuG8jdomWJns"
   },
   "source": [
    "## Reading from List of words"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "metadata": {
    "id": "LXx8qA8MWJIU"
   },
   "outputs": [],
   "source": [
    "data = pd.read_csv(\"wordleanswers.csv\")"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Function to check if the Word is Valid"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "metadata": {},
   "outputs": [],
   "source": [
    "def is_word(word):\n",
    "    try:\n",
    "        output = dictionary.meaning(word, disable_errors=True)\n",
    "    except:\n",
    "        return False\n",
    "    else:\n",
    "        return bool(output)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {
    "id": "xeW1WeGyWUy0"
   },
   "source": [
    "## Initializing Variables and Lists"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "metadata": {
    "id": "vIhGL0aeWX62"
   },
   "outputs": [],
   "source": [
    "word = data.sample().iloc[0][\"Word\"][1:]\n",
    "keyboard = {\"q\":\"\",\"w\":\"\",\"e\":\"\",\"r\":\"\",\"t\":\"\",\"y\":\"\",\"u\":\"\",\"i\":\"\",\"o\":\"\",\"p\":\"\",\"a\":\"\",\"s\":\"\",\"d\":\"\",\"f\":\"\",\"g\":\"\",\"h\":\"\",\"j\":\"\",\"k\":\"\",\"l\":\"\",\"z\":\"\",\"x\":\"\",\"c\":\"\",\"v\":\"\",\"b\":\"\",\"n\":\"\",\"m\":\"\"}\n",
    "fin = False\n",
    "grid_memory = []\n",
    "col={}\n",
    "user_list = []"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {
    "id": "dNuQcu23WjOk"
   },
   "source": [
    "## Function to Assign Colors to a Single Row of the Grid"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "metadata": {
    "id": "aZxeLa_XWcXr"
   },
   "outputs": [],
   "source": [
    "def assign_colors_grid(word, user_input, col, keyboard):\n",
    "    col = {0:\"\", 1:\"\", 2:\"\", 3:\"\", 4:\"\"}\n",
    "    for i in range(5):\n",
    "        if(user_input[i] not in word):\n",
    "            col[i] = \"on_grey\"\n",
    "            keyboard[user_input[i]] = \"on_grey\"\n",
    "\n",
    "    for i in range(5):\n",
    "        if(user_input[i] == word[i]):\n",
    "            col[i] = \"on_green\"\n",
    "            keyboard[user_input[i]] = \"on_green\"\n",
    "\n",
    "    for i in range(5):\n",
    "        if(col[i] == \"on_green\" or col[i] == \"on_grey\"):\n",
    "            continue\n",
    "        count = 0\n",
    "        for j in range(5):\n",
    "            if(user_input[j] == user_input[i] and col[j] != \"on_green\"):\n",
    "                count += 1\n",
    "        let_count = word.count(user_input[i])\n",
    "        j = 0 \n",
    "        while(count > 0 and let_count > 0):\n",
    "            if(user_input[j] == user_input[i] and col[j] == \"\" ):\n",
    "                col[j] = \"on_yellow\"\n",
    "                let_count -= 1\n",
    "                count -= 1\n",
    "            j += 1\n",
    "            if(j == 5):\n",
    "                break\n",
    "        for j in range(5):\n",
    "            if(user_input[j] == user_input[i] and col[j] == \"\"):\n",
    "                col[j] = \"on_grey\"\n",
    "        \n",
    "        if(user_input[i] != word[i] and user_input[i] in word):\n",
    "            if(keyboard[user_input[i]] == \"on_green\"):\n",
    "                continue\n",
    "            else:\n",
    "                keyboard[user_input[i]] = \"on_yellow\"\n",
    "\n",
    "        for i in range(5):\n",
    "            if(col[i] == \"on_grey\"):\n",
    "                keyboard[user_input[i]] = \"on_grey\"\n",
    "\n",
    "    grid_memory.append(col)\n",
    "    return col"
   ]
