   "cell_type": "code",
   "execution_count": 22,
   "metadata": {
    "id": "wDC25OaCXGzL"
   },
   "outputs": [],
   "source": [
    "def print_full_grid(i, grid_memory, user_list):\n",
    "    for k in range(i+1):\n",
    "        for m in range(5):\n",
    "            print(colored(user_list[k][m], \"white\", grid_memory[k][m]), end=\" \")\n",
    "        print(\"\\n\")\n",
    "    for k in range(5-i):\n",
    "        print(\"_ _ _ _ _\")"
   ]
