    "for i in range(6):\n",
    "    if(i==0):\n",
    "        print(\"_ _ _ _ _\\n_ _ _ _ _\\n_ _ _ _ _\\n_ _ _ _ _\\n_ _ _ _ _\\n_ _ _ _ _\\n\")\n",
    "    print_keyboard(keyboard)\n",
    "    print(\"\\n\")\n",
    "    user_input = ''\n",
    "    valid_word = False\n",
    "    while((len(user_input) != 5) or is_word(user_input) is False):\n",
    "        user_input = input()\n",
    "        print(\"\\n\")\n",
    "        if(user_input == word):\n",
    "            fin = True\n",
    "        if(fin is True):\n",
    "            break\n",
    "        valid_word = is_word(user_input)\n",
    "        if(len(user_input) != 5):\n",
    "            print(\"Not a 5 letter word!\\n\")\n",
    "        if(is_word(user_input) is False):\n",
    "            print(\"Not a valid word!\\n\")\n",
    "    print(\"\\n-----------------------\")\n",
    "    if(fin is True):\n",
    "        user_list.append(user_input)\n",
    "        col = assign_colors_grid(word, user_input, col, keyboard)\n",
    "        print_full_grid(i, grid_memory, user_list)\n",
    "        print(\"\\n\")\n",
    "        print(\"-----------------------\")\n",
    "        print(\"\\n\")\n",
    "        print_keyboard(keyboard)\n",
    "        print(\"\\n\")\n",
    "        print(\"Correct! \", i+1, \"/ 6\")\n",
    "        print_squares(i+1, grid_memory)\n",
    "        break\n",
    "    user_list.append(user_input)\n",
    "    col = assign_colors_grid(word, user_input, col, keyboard)\n",
    "    print_full_grid(i, grid_memory, user_list)\n",
    "    print(\"\\n\")\n",
    "\n",
    "if(fin is False):\n",
    "    print_squares(i+1,grid_memory)\n",
    "    print(\"\\nSolution:\", word, \"\\nX / 6\")"
   ]
