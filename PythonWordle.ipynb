    "#importing dictionary library\n",
    "from PyDictionary import PyDictionary\n",
    "dictionary = PyDictionary()\n",
    "\n",
    "#importing text color library\n",
    "from termcolor import colored\n",
    "\n",
    "#importing wordle list\n",
    "import pandas as pd\n",
    "\n",
    "data = pd.read_csv(\"wordleanswers.csv\")\n",
    "\n",
    "#function to check if the word is valid\n",
    "def is_word(word):\n",
    "    try:\n",
    "        output = dictionary.meaning(word, disable_errors=True)\n",
    "    except:\n",
    "        return False\n",
    "    else:\n",
    "        return bool(output)\n",
    "\n",
    "#initializing variables and lists\n",
    "word = data.sample().iloc[0][\"Word\"][1:]\n",
    "keyboard = {\"q\":\"\",\"w\":\"\",\"e\":\"\",\"r\":\"\",\"t\":\"\",\"y\":\"\",\"u\":\"\",\"i\":\"\",\"o\":\"\",\"p\":\"\",\"a\":\"\",\"s\":\"\",\"d\":\"\",\"f\":\"\",\"g\":\"\",\"h\":\"\",\"j\":\"\",\"k\":\"\",\"l\":\"\",\"z\":\"\",\"x\":\"\",\"c\":\"\",\"v\":\"\",\"b\":\"\",\"n\":\"\",\"m\":\"\"}\n",
    "fin = False\n",
    "grid_memory = []\n",
    "col={}\n",
    "user_list = []\n",
    "\n",
    "def assign_colors_grid(word, user_input, col, keyboard):\n",
    "    col = {0:\"\", 1:\"\", 2:\"\", 3:\"\", 4:\"\"}\n",
    "    for i in range(5):\n",
    "        if(user_input[i] not in word):\n",
    "            col[i] = \"on_grey\"\n",
    "            keyboard[user_input[i]] = \"on_grey\"\n",
    "\n",
    "    for i in range(5):\n",
    "        if(user_input[i] == word[i]):\n",
    "            col[i] = \"on_green\"\n",
    "            keyboard[user_input[i]] = \"on_green\"\n",
    "\n",
    "    for i in range(5):\n",
    "        if(col[i] == \"on_green\" or col[i] == \"on_grey\"):\n",
    "            continue\n",
    "        count = 0\n",
    "        for j in range(5):\n",
    "            if(user_input[j] == user_input[i] and col[j] != \"on_green\"):\n",
    "                count += 1\n",
    "        let_count = word.count(user_input[i])\n",
    "        j = 0 \n",
    "        while(count > 0 and let_count > 0):\n",
    "            if(user_input[j] == user_input[i] and col[j] == \"\" ):\n",
    "                col[j] = \"on_yellow\"\n",
    "                let_count -= 1\n",
    "                count -= 1\n",
    "            j += 1\n",
    "            if(j == 5):\n",
    "                break\n",
    "        for j in range(5):\n",
    "            if(user_input[j] == user_input[i] and col[j] == \"\"):\n",
    "                col[j] = \"on_grey\"\n",
    "        \n",
    "        if(user_input[i] != word[i] and user_input[i] in word):\n",
    "            if(keyboard[user_input[i]] == \"on_green\"):\n",
    "                continue\n",
    "            else:\n",
    "                keyboard[user_input[i]] = \"on_yellow\"\n",
    "\n",
    "        for i in range(5):\n",
    "            if(col[i] == \"on_grey\"):\n",
    "                keyboard[user_input[i]] = \"on_grey\"\n",
    "\n",
    "    grid_memory.append(col)\n",
    "    return col\n",
    "\n",
    "def print_keyboard(keyboard):\n",
    "    alph_list = list(keyboard)\n",
    "    print(\"     Keyboard\")\n",
    "    for i in range(26):\n",
    "        if(i == 10 or i == 19):\n",
    "            print(\"\\n\")\n",
    "        if(keyboard[alph_list[i]] != ''):\n",
    "            print((colored(alph_list[i], \"white\", keyboard[alph_list[i]])), end=\" \")\n",
    "        else:\n",
    "            print(alph_list[i], end=\" \")\n",
    "\n",
    "def print_full_grid(i, grid_memory, user_list):\n",
    "    for k in range(i+1):\n",
    "        for m in range(5):\n",
    "            print(colored(user_list[k][m], \"white\", grid_memory[k][m]), end=\" \")\n",
    "        print(\"\\n\")\n",
    "    for k in range(5-i):\n",
    "        print(\"_ _ _ _ _\")\n",
    "\n",
    "def print_squares(x, grid_memory):\n",
    "    for i in range(x):\n",
    "        #print(\"\\n\")\n",
    "        for j in range(5):\n",
    "            if(grid_memory[i][j] == \"on_green\"):\n",
    "                print(\"游릴\", end=\"\")\n",
    "            elif(grid_memory[i][j] == \"on_yellow\"):\n",
    "                print(\"游릳\", end=\"\")\n",
    "            elif(grid_memory[i][j] == \"on_grey\"):\n",
    "                print(\"拘뜦", end=\"\")\n",
    "            if(j == 4):\n",
    "                print(\"\\n\")\n",
    "\n",
    "\n",
    "#main loop\n",
    "for i in range(6):\n",
    "    if(i==0):\n",
    "        print(\"_ _ _ _ _\\n_ _ _ _ _\\n_ _ _ _ _\\n_ _ _ _ _\\n_ _ _ _ _\\n_ _ _ _ _\\n\")\n",
    "    print_keyboard(keyboard)\n",
    "    print(\"\\n\")\n",
    "    user_input = ''\n",
    "    valid_word = False\n",
    "    while((len(user_input) != 5) or is_word(user_input) is False):\n",
    "        user_input = input()\n",
    "        print(\"\\n\")\n",
    "        if(user_input == word):\n",
    "            fin = True\n",
    "        if(fin is True):\n",
    "            break\n",
    "        valid_word = is_word(user_input)\n",
    "        if(len(user_input) != 5):\n",
    "            print(\"Not a 5 letter word!\\n\")\n",
    "        if(is_word(user_input) is False):\n",
    "            print(\"Not a valid word!\\n\")\n",
    "    print(\"\\n-----------------------\")\n",
    "    if(fin is True):\n",
    "        user_list.append(user_input)\n",
    "        col = assign_colors_grid(word, user_input, col, keyboard)\n",
    "        print_full_grid(i, grid_memory, user_list)\n",
    "        print(\"\\n\")\n",
    "        print(\"-----------------------\")\n",
    "        print(\"\\n\")\n",
    "        print_keyboard(keyboard)\n",
    "        print(\"\\n\")\n",
    "        print(\"Correct! \", i+1, \"/ 6\")\n",
    "        print_squares(i+1, grid_memory)\n",
    "        break\n",
    "    user_list.append(user_input)\n",
    "    col = assign_colors_grid(word, user_input, col, keyboard)\n",
    "    print_full_grid(i, grid_memory, user_list)\n",
    "    print(\"\\n\")\n",
    "\n",
    "if(fin is False):\n",
    "    print(\"\\nSolution:\", word, \"\\n\")\n",
    "    print(\"\\nX / 6\")\n",
    "    print_squares(i+1,grid_memory)"
   ]
  },
