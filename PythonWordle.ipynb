   "source": [
    "def print_squares(x, grid_memory):\n",
    "    for i in range(x):\n",
    "        #print(\"\\n\")\n",
    "        for j in range(5):\n",
    "            if(grid_memory[i][j] == \"on_green\"):\n",
    "                print(\"游릴\", end=\"\")\n",
    "            elif(grid_memory[i][j] == \"on_yellow\"):\n",
    "                print(\"游릳\", end=\"\")\n",
    "            elif(grid_memory[i][j] == \"on_grey\"):\n",
    "                print(\"拘뜦", end=\"\")\n",
    "            if(j == 4):\n",
    "                print(\"\\n\")"
